{
  "noEmitErrors": true,
  "exclude": ["node_modules"],
  "include": ["sagas/index.ts","stores/index.ts","reducers/index.ts"],
  "compilerOptions": {
    "baseUrl": ".", 
    "experimentalDecorators": true,
    "target": "ES5",
    "lib": ["DOM", "ES6"],
    "jsx": "react",
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "removeComments": true,
    "outDir": "dist"  }
}